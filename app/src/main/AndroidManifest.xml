<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.ac.ui.ft.personalizedobdscan">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.airfilter.AirFilterActivity"
            android:label="Air Filter Analysis"
            android:theme="@style/AppTheme"/>
        <activity
            android:name=".views.braking.BrakingAnalysisActivity"
            android:label="Braking Analysis"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".views.fuel.FuelActivity"
            android:label="Fuel Analysis"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".views.EmissionSystemActivity"
            android:label="Emmision Analysis"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".views.OilSystemActivity"
            android:label="Oil Analysis"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".views.AccuSystemActivity"
            android:label="Battery Analysis"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".views.LauncherActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".views.RegisterActivity"
            android:label="Register"
            android:theme="@style/AppTheme" />
        <activity android:name=".views.HomeActivity" />
        <activity
            android:name=".views.AnalysisActivity"
            android:label="Analysis"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".views.maintenance.MaintenanceJournalActivity"
            android:label="Maintenance Journal"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".views.maintenance.MaintenanceDamageListActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".views.maintenance.JournalActivity"
            android:theme="@style/AppTheme" />
    </application>

</manifest>